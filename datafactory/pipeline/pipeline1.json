{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Get and store Station Ids",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "station/*"
					},
					{
						"name": "Destination",
						"value": "landing-zone/@{substring(pipeline().TriggerTime, 0, 20)}/@{concat('StationIds ', pipeline().RunId) }"
					}
				],
				"typeProperties": {
					"source": {
						"type": "XmlSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"maxConcurrentConnections": 1,
							"requestMethod": "GET",
							"requestTimeout": ""
						},
						"formatSettings": {
							"type": "XmlReadSettings",
							"validationMode": "none",
							"namespaces": true
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"maxConcurrentConnections": 1
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['stations']['station'][0]['@name']"
								},
								"sink": {
									"name": "@@name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['stations']['station'][0]['@eva']"
								},
								"sink": {
									"name": "@@eva",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['stations']['station'][0]['@ds100']"
								},
								"sink": {
									"name": "@@ds100",
									"type": "String"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "StationIdsRaw",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "StationIdsCSV",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "bahncollector"
		},
		"annotations": []
	}
}